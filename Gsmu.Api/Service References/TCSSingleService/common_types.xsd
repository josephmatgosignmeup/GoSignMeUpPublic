<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xsd1="http://fms.treas.gov/tcs/schemas" xmlns="http://fms.treas.gov/" elementFormDefault="qualified" targetNamespace="http://fms.treas.gov/tcs/schemas" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="level3.xsd" />
  <xs:simpleType name="email_address">
    <xs:annotation>
      <xs:documentation>Email address that is associated with the payment</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSEmail">
      <xs:maxLength value="80" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="account_holder_name">
    <xs:annotation>
      <xs:documentation>Full name of person holding the account, possibly modified during the Pay.gov session 
				Required if the agency is sending account data to Pay.gov; otherwise optional.
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSStringPlus">
      <xs:minLength value="1" />
      <xs:maxLength value="80" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="account_number">
    <xs:annotation>
      <xs:documentation>The account number supplied with the transaction</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSAccountNumber">
      <xs:minLength value="3" />
      <xs:maxLength value="17" />
      <xs:whiteSpace value="preserve" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="account_type">
    <xs:annotation>
      <xs:documentation>
				Applicable if payment_type is ACH. Must be one of allowable enumerations.
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="BusinessChecking" />
      <xs:enumeration value="BusinessSavings" />
      <xs:enumeration value="PersonalChecking" />
      <xs:enumeration value="PersonalSavings" />
      <xs:enumeration value="GeneralLedger" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="agency_id">
    <xs:annotation>
      <xs:documentation>Unique identifer of the agency, as it was assigned by Pay.gov</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSNumericString">
      <xs:maxLength value="21" />
      <xs:minLength value="0" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="agency_tracking_id">
    <xs:annotation>
      <xs:documentation>
				Unique identifier controlled by the agency and supplied with the transaction.
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSStringPlus">
      <xs:maxLength value="21" />
      <xs:minLength value="1" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="app_batch_id">
    <xs:annotation>
      <xs:documentation>
				Unique identifier controlled by the agency and supplied with the batch.
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSStringPlus">
      <xs:minLength value="1" />
      <xs:maxLength value="32" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="approval_code">
    <xs:annotation>
      <xs:documentation>Approval Code exactly as received from the Plastic Card authorization provider.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="1" />
      <xs:maxLength value="64" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="approvalCodeResponseType">
    <xs:annotation>
      <xs:documentation>Approval Code set in response. Please note: this value can be an empty string.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="64" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="auth_response_code">
    <xs:annotation>
      <xs:documentation>Authorization Response Code, exactly as received from the authorization service provider.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="2" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="auth_response_text">
    <xs:annotation>
      <xs:documentation>Authorization Response Text, exactly as received from the authorization service provider.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="16" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="avs_response_code">
    <xs:annotation>
      <xs:documentation>Address Verification Service result exactly as received from the Plastic Card authorization provider.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="20" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="batch_status">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Duplicated" />
      <xs:enumeration value="Failed" />
      <xs:enumeration value="Received" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="batch_return_code">
    <xs:annotation>
      <xs:documentation>Numeric String</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSNumericString">
      <xs:maxLength value="4" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="batch_return_detail">
    <xs:annotation>
      <xs:documentation>Detailed message elaborating on return_code.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="256" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="billing_address">
    <xs:annotation>
      <xs:documentation>Billing address supplied with the transaction</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSStringPlus">
      <xs:maxLength value="80" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="billing_address_2">
    <xs:annotation>
      <xs:documentation>Billing address line 2 supplied with the transaction</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSStringPlus">
      <xs:maxLength value="80" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="billing_city">
    <xs:annotation>
      <xs:documentation>Billing city supplied with the transaction</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSAlphaNumericPlus">
      <xs:maxLength value="40" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="billing_state">
    <xs:annotation>
      <xs:documentation>Billing state or province of address</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSAlphaNumericPlus">
      <xs:maxLength value="02" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="billing_zip">
    <xs:annotation>
      <xs:documentation>Billing zip code of address</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSAlphaNumericPlus">
      <xs:maxLength value="20" />
    </xs:restriction>
  </xs:simpleType>
