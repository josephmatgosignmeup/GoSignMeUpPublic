  <xs:complexType name="address">
    <xs:sequence>
      <xs:element name="billing_address" type="xsd1:billing_address" />
      <xs:element minOccurs="0" name="billing_address_2" type="xsd1:billing_address_2" />
      <xs:element name="billing_city" type="xsd1:billing_city" />
      <xs:element name="billing_state" type="xsd1:billing_state" />
      <xs:element name="billing_zip" type="xsd1:billing_zip" />
      <xs:element name="billing_country" type="xsd1:billing_country" />
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="card_security_code">
    <xs:annotation>
      <xs:documentation>The 3 or 4 digit number listed on the back/front of the cardholder card by the signature line.  Applicable 	if payment_type is PlasticCard</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="([0-9]{3,4}){0,1}" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="check_number">
    <xs:annotation>
      <xs:documentation>The check number that can be assigned to the withdrawal. 
				Applicable if payment_type is ACH.
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSNumericString">
      <xs:minLength value="0" />
      <xs:maxLength value="8" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="credit_card_expiration_date">
    <xs:annotation>
      <xs:documentation>Text string representation of the expiration date of the Plastic Card.  The format of this value shall be: YYYY-DD. </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:gYearMonth" />
  </xs:simpleType>
  <xs:simpleType name="credit_card_track2">
    <xs:annotation>
      <xs:documentation>
				Track2 data of credit card swipe.  Applicable if payment_type is plastic_card.
				As per requirements there is no restriction on credit card track 2 data.
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="256" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="custom_fields">
    <xs:annotation>
      <xs:documentation>Customized fields that can be passed by the agency to capture agency management data or can be exposed to the user during the transaction.
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="custom_field_1" type="xsd1:customFieldValue" />
      <xs:element minOccurs="0" name="custom_field_2" type="xsd1:customFieldValue" />
      <xs:element minOccurs="0" name="custom_field_3" type="xsd1:customFieldValue" />
      <xs:element minOccurs="0" name="custom_field_4" type="xsd1:customFieldValue" />
      <xs:element minOccurs="0" name="custom_field_5" type="xsd1:customFieldValue" />
      <xs:element minOccurs="0" name="custom_field_6" type="xsd1:customFieldValue" />
      <xs:element minOccurs="0" name="custom_field_7" type="xsd1:customFieldValue" />
      <xs:element minOccurs="0" name="custom_field_8" type="xsd1:customFieldValue" />
      <xs:element minOccurs="0" name="custom_field_9" type="xsd1:customFieldValue" />
      <xs:element minOccurs="0" name="custom_field_10" type="xsd1:customFieldValue" />
      <xs:element minOccurs="0" name="custom_field_11" type="xsd1:customFieldValue" />
      <xs:element minOccurs="0" name="custom_field_12" type="xsd1:customFieldValue" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="classification">
    <xs:annotation>
      <xs:documentation>Classification that can be passed by the agency to capture TAS/BETC amounts for each transaction.
			</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="classification_data" type="xsd1:classificationDataType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="query_custom_fields">
    <xs:annotation>
      <xs:documentation>Customized fields that can be passed by the agency to capture agency management data or can be exposed to the user during the transaction.
	</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="custom_field_1" type="xsd1:CustomCollectionLabelAndFieldType" />
      <xs:element minOccurs="0" name="custom_field_2" type="xsd1:CustomCollectionLabelAndFieldType" />
      <xs:element minOccurs="0" name="custom_field_3" type="xsd1:CustomCollectionLabelAndFieldType" />
      <xs:element minOccurs="0" name="custom_field_4" type="xsd1:CustomCollectionLabelAndFieldType" />
      <xs:element minOccurs="0" name="custom_field_5" type="xsd1:CustomCollectionLabelAndFieldType" />
      <xs:element minOccurs="0" name="custom_field_6" type="xsd1:CustomCollectionLabelAndFieldType" />
      <xs:element minOccurs="0" name="custom_field_7" type="xsd1:CustomCollectionLabelAndFieldType" />
      <xs:element minOccurs="0" name="custom_field_8" type="xsd1:CustomCollectionLabelAndFieldType" />
      <xs:element minOccurs="0" name="custom_field_9" type="xsd1:CustomCollectionLabelAndFieldType" />
      <xs:element minOccurs="0" name="custom_field_10" type="xsd1:CustomCollectionLabelAndFieldType" />
      <xs:element minOccurs="0" name="custom_field_11" type="xsd1:CustomCollectionLabelAndFieldType" />
      <xs:element minOccurs="0" name="custom_field_12" type="xsd1:CustomCollectionLabelAndFieldType" />
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="csc_result">
    <xs:annotation>
      <xs:documentation>Card Verification Value Result Code exactly as received by the authorization provider</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="1" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="debit_voucher_number">
    <xs:annotation>
      <xs:documentation>Assigned to ACH items that are either returned or retired.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSAlphaNumeric">
      <xs:maxLength value="10" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="deposit_ticket_number">
    <xs:annotation>
      <xs:documentation>Assigned to ACH items that are settled.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSAlphaNumeric">
      <xs:maxLength value="10" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="effective_date">
    <xs:annotation>
      <xs:documentation>Effective date of the ACH collection.  In the majority of cases this will match the payment_date supplied wth the request.  Please see the TCSDate for format.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSDate" />
  </xs:simpleType>
  <xs:simpleType name="masked_account_number">
    <xs:annotation>
      <xs:documentation>
				Account Number returned in results.  The system shall show last 4 digits preceded by 12 asterisks.
				USED FOR OUTPUT ONLY!
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSMaskedAccountNumber" />
  </xs:simpleType>
  <xs:simpleType name="order_id">
    <xs:annotation>
      <xs:documentation>Level II data field for a credit card transaction.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSStringPlus">
      <xs:minLength value="0" />
      <xs:maxLength value="17" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="order_tax_amount">
    <xs:annotation>
      <xs:documentation>Level II data field for a credit card transaction.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:minInclusive value="0" />
      <xs:maxExclusive value="9999999999999999999" />
      <xs:fractionDigits value="2" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="paygov_tracking_id">
    <xs:annotation>
      <xs:documentation>Pay.gov-generated unique tracking identifier</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSAlphaNumeric">
      <xs:maxLength value="21" />
      <xs:minLength value="1" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="payment_date">
    <xs:restriction base="xsd1:TCSDate" />
  </xs:simpleType>
  <xs:simpleType name="payment_type">
    <xs:restriction base="xsd1:TCSString">
      <xs:enumeration value="ACH" />
      <xs:enumeration value="PlasticCard" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="recurring_payment_number">
    <xs:annotation>
      <xs:documentation>Payment number of a recurring ACH collection</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="9999" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="recurring_payment_number_nozero">
    <xs:annotation>
      <xs:documentation>Payment number of a recurring ACH collection</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="9999" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="recurring_payment_frequency">
    <xs:annotation>
      <xs:documentation>Frequency of a recurring ACH collection.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="" />
      <xs:enumeration value="OneTime" />
      <xs:enumeration value="Weekly" />
      <xs:enumeration value="Biweekly" />
      <xs:enumeration value="Monthly" />
      <xs:enumeration value="Quarterly" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="payment_frequency">
    <xs:annotation>
      <xs:documentation>Frequency of an ACH collection.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="OneTime" />
      <xs:enumeration value="Weekly" />
      <xs:enumeration value="Biweekly" />
      <xs:enumeration value="Monthly" />
      <xs:enumeration value="Quarterly" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="return_code">
    <xs:annotation>
      <xs:documentation>Four digit numeric code that specifies the status of this specific request. Can be blank if it is a transaction that was not settled using TCS.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSNumericString">
      <xs:maxLength value="4" />
      <xs:minLength value="0" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="return_detail">
    <xs:annotation>
      <xs:documentation>Code that, along with return_code, gives the agency system a complete understanding of the error.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSString">
      <xs:maxLength value="256" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="return_reason_code">
    <xs:annotation>
      <xs:documentation>Reason why the ACH transaction was returned.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="100" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="routing_transit_number">
    <xs:annotation>
      <xs:documentation>Routing Transit Number associated with the account.</xs:documentation>
    </xs:annotation>
