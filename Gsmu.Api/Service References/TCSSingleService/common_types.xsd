    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="sec_code">
    <xs:annotation>
      <xs:documentation>Optional parameter that may be sent in request for an ACH collection if the check type is not Business.</xs:documentation>
    </xs:annotation>
