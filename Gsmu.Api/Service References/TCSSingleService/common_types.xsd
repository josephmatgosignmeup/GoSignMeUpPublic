  <xs:simpleType name="CustomCollectionFieldType">
    <xs:annotation>
      <xs:documentation>Customized field that can be passed by the agency to capture agency management data.
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSString">
      <xs:minLength value="1" />
      <xs:maxLength value="255" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TCSAccountNumber">
    <xs:annotation>
      <xs:documentation>
				This simple type is used by account_number nodes for defining both Ach and Plastic Card account numbers.
				Due to the fact that the Nacha allowable list include all characters that are currently allowed for Plastic Card numbers, 
				the regular expression pattern contained herein is defines the Nacha list.
				The decision to make this data type less restrictive was an attempt to minimize potential future changes causing
				backward compatibility issues.
				PLEASE NOTE: 
							XSD Regular Expression special characters list 
							[]\^.|?*+(){}-
							These characters must be escaped by reverse slant ( \ )
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="256" />
      <xs:pattern value="[a-zA-Z0-9\s\[\]\\\^\.\|\?\*\+\(\)\{\}$`~!@#%_= ;:',/\-]*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TCSMaskedAccountNumber">
    <xs:restriction base="xs:string">
      <xs:maxLength value="16" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TCSAlphaNumeric">
    <xs:restriction base="xs:string">
      <xs:pattern value="[a-zA-Z0-9\s]*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TCSAlphaNumericPlus">
    <xs:restriction base="xs:string">
      <xs:pattern value="[a-zA-Z0-9\(\)\s\.\-_']*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="classification_id">
    <xs:annotation>
      <xs:documentation>Classification Id can only contain A-Z, a-z, 0-9 (alphanumerics), only one contiguous embedded space (can contain more than one space, just not together, or at the beginning or end), and no leading and/or trailing spaces.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="0" />
      <xs:maxLength value="24" />
      <xs:pattern value="[a-zA-Z0-9]+(\s[a-zA-Z0-9]+)*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TCSDate">
    <xs:annotation>
      <xs:documentation>The date is specified in the following form "YYYY-MM-DD" where:
				* YYYY indicates the year
				* MM indicates the month
				* DD indicates the day
				Note: All components are required!			
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:date" />
  </xs:simpleType>
  <xs:simpleType name="TCSDuration">
    <xs:annotation>
      <xs:documentation>The date is specified in the following form "HH:MM:SS" where:
				* HH is hours, max 24
				* MM is minutes, max 59
				* SS is seconds, max 59
				Note: All components are required!
				The maximum value is 24:00:00
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:time" />
  </xs:simpleType>
  <xs:simpleType name="TCSDatetime">
    <xs:annotation>
      <xs:documentation>The dateTime is specified in the following form "YYYY-MM-DDThh:mm:ss" where:
				* YYYY indicates the year
				* MM indicates the month
				* DD indicates the day
				* T indicates the start of the required time section
				* hh indicates the hour
				* mm indicates the minute
				* ss indicates the second
				Note: All components are required!
			</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:dateTime" />
  </xs:simpleType>
  <xs:simpleType name="TCSEmail">
    <xs:restriction base="xs:string">
      <xs:pattern value="([a-zA-Z0-9\-_']+(\.[a-zA-Z0-9\-_']+)*@([a-zA-Z0-9\-_]+\.)+[a-zA-Z]{2,7})?" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TCSString">
    <xs:restriction base="xs:string">
      <xs:pattern value="([a-zA-Z0-9\.\s\-\+\(\),:@_#$'!=/&amp;])*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TCSStringPlus">
    <xs:restriction base="xs:string">
      <xs:pattern value="([a-zA-Z0-9\.\s\-\(\),:@_#'/&amp;])*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="TCSNumericString">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]*" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="paygov_date_time">
    <xs:annotation>
      <xs:documentation>format is mmddyyyyThhmmss</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSDatetime" />
  </xs:simpleType>
  <xs:simpleType name="optionalPaygovTrackingIdType">
    <xs:annotation>
      <xs:documentation>Unique identifier generated by the Paygov system. Refined for response to allow for empty value.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSAlphaNumeric">
      <xs:maxLength value="21" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="optionalAgencyTrackingIdType">
    <xs:annotation>
      <xs:documentation>Unique identifier generated by agency system. Refined for response to allow for empty value.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSString">
      <xs:maxLength value="21" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>