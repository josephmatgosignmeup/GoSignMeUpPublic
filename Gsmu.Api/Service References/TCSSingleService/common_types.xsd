      <xs:enumeration value="CCD" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tcs_app_id">
    <xs:annotation>
      <xs:documentation>The agency-assigned identifier that Pay.gov can use to look up the applicable collection options.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xsd1:TCSAlphaNumericPlus">
      <xs:minLength value="1" />
      <xs:maxLength value="21" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="total_recurring_payments">
    <xs:annotation>
      <xs:documentation>Number of recurring payments.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="9999" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="total_installments">
    <xs:annotation>
      <xs:documentation>Number of installments</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:integer">
