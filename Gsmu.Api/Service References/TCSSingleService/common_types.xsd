    </xs:annotation>
    <xs:restriction base="xsd1:TCSDatetime" />
  </xs:simpleType>
  <xs:simpleType name="transaction_status">
    <xs:annotation>
      <xs:documentation>status of collection processing</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="" />
      <xs:enumeration value="Canceled" />
      <xs:enumeration value="Failed" />
      <xs:enumeration value="Received" />
      <xs:enumeration value="Retired" />
      <xs:enumeration value="Settled" />
      <xs:enumeration value="Success" />
      <xs:enumeration value="PartialAuth" />
      <xs:enumeration value="Scheduled" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="authorized_amount">
    <xs:annotation>
      <xs:documentation>Used to support partial authorizations.  This value would represent the actual authorized or approved amount for a authorization request. </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:decimal">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="999999999999999999999" />
      <xs:fractionDigits value="2" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="remaining_balance">
    <xs:annotation>
