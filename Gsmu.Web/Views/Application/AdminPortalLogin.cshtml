@using Gsmu.Api.Data
@{
    Layout = null;
}
<html>
<head>
    <title></title>
    <meta http-equiv="cache-control" content="max-age=0" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
    <meta http-equiv="pragma" content="no-cache" />
</head>
<body>
    <form id="send" method="post" action="@(Settings.Instance.GetMasterInfo4().AspSiteRootUrl + "index.asp?redirect=" + Request.QueryString["redirect"])">

        <input type="hidden" name="name" value="@WebConfiguration.AdminUser" />
        <input type="hidden" name="hname" value="1" />
        <input type="hidden" name="pass" value="@WebConfiguration.AdminPassword" />
        <input type="hidden" name="hpass" value="1" />
        <input type="hidden" name="Submit" value="Submit" />
    </form>
    <script type="text/javascript">
        document.getElementById('send').submit();
    </script>
</body>
</html>
