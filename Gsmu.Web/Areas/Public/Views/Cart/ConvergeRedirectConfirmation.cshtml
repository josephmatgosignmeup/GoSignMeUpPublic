@{
    ViewBag.Title = "Confirmation";
}
<br />
<input type="hidden" value="@Html.Raw(ViewBag.OrderNumber)" id="OrderNumber" style="width:530px;" class="textbox-cc" />
<input type="hidden" value="@Html.Raw(ViewBag.OrderTotal)" id="OrderTotal" style="width:530px;" class="textbox-cc" />
<div id="confirmationprint">
    Processing Receipt...
</div>
<br />
<script>
    var orderNumber = '@ViewBag.OrderNumber';
    Cart.prototype.ShowConfirmationReceipt(orderNumber);
</script>